package trabalhoFinal;

public class MainSistemaInterno {

    public static void main(String[] args) {
        // Criando uma conta corrente
        Conta conta1 = new Conta("123654789", 200.00, "1", TipoConta.CONTA_CORRENTE);
        System.out.println("Conta 1: Tipo " + conta1.getTipo() + ", Saldo: " + conta1.getSaldo());

        // Criando uma conta poupan√ßa
        Conta conta2 = new Conta("1323654789", 150.00, "2", TipoConta.CONTA_POUPANCA);
        System.out.println("Conta 2: Tipo " + conta2.getTipo() + ", Saldo: " + conta2.getSaldo());
    }
}


