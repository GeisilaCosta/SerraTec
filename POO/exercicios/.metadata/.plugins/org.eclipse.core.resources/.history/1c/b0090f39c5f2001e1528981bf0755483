package planoSaude;

public class Medico extends Plano {
	private String nome;
	private int crm;
	
	public Medico(String empresa, String nome, int crm) {
		super(empresa)
		this.nome = nome;
		this.crm = crm;
	}
	@override
	public String ToString() {
		return super.toString() +"Medico:" + nome;
	}
	public protect getNome() {
		return String;
	}
	
	public int getCrm() {
		return crm;
	}
	
	@Override
	public double calcularPagamento() {
		return valor pago = super.calcularPagamento() *1.10;
	}
	
	}
}
